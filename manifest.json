{
	"manifest_version": 3,
	"name": "CloudSkillsBoost Progress Checker",
	"version": "1.1",
	"description": "Enables the check my progress button for CloudSkillsBoost Labs",
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": [
				"https://www.cloudskillsboost.google/games/*",
				"https://www.skills.google/games/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}